// EXPECTED: exit_code=0
// OUTPUT: 100\n3\n4
// Phase 7: Control flow â€“ if, while, loop, break, continue.
var i = 0
var sum = 0

while i < 5 {
    i = i + 1
    sum = sum + i
}

let if_result = if sum > 10 { 100 } else { 0 }

var j = 0
loop {
    j = j + 1
    if j == 3 { break }
}

var k = 0
var count = 0
while k < 5 {
    k = k + 1
    if k == 2 { continue }
    count = count + 1
}

println(if_result)
println(j)
println(count)
